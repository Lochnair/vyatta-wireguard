type: u32
help: Interface cost
syntax:expression: $VAR(@) >= 1 && $VAR(@) <= 65535; "Must be between 1-65535"
val_help: u32:1-65535; OSPF interface cost
update:expression: "vtysh-set -i $VAR(../../../@) \
                              -c \"interface $VAR(../../../@)\" \
                              -c \"ip ospf cost $VAR(@)\"; "
delete:expression: "vtysh-set -i $VAR(../../../@) --noerr \
                              -c \"interface $VAR(../../../@)\" \
                              -c \"no ip ospf cost\"; "
